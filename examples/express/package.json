{
  "name": "@totem-otp/example-express",
  "version": "0.0.1",
  "description": "Example integration of TotemOTP with Redis storage and webhook delivery",
  "main": "dist/main.js",
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist/",
    "test": "jest",
    "test:integration": "jest --testPathPattern=integration",
    "example": "ts-node src/main.ts",
    "start": "node dist/main.js",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:redis": "docker-compose up -d redis",
    "dev:docker": "npm run docker:redis && npm run dev",
    "test:docker": "npm run docker:redis && npm run test:integration"
  },
  "dependencies": {
    "@totem-otp/core": "*",
    "@totem-otp/storage-redis": "*",
    "@totem-otp/delivery-webhook": "*",
    "express": "^4.18.2",
    "redis": "^4.6.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@types/supertest": "^2.0.16",
    "jest": "^29.5.0",
    "redis-memory-server": "^0.10.0",
    "supertest": "^6.3.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0",
    "nock": "^13.4.0"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": [
      "**/__tests__/**/*.test.ts"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/**/*.d.ts",
      "!src/**/__tests__/**"
    ]
  }
}
